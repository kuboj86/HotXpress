<Page x:Class="HotXpress.Menu_Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HotXpress"
      mc:Ignorable="d" 
      d:DesignHeight="1792" d:DesignWidth="828"
      Title="Menu_Page">

    <Grid Width="828" Height="1792">
        <Frame x:Name="MenuFrame" Content="" Background="#FF8BC77D" Height="1792" Width="828"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="10,10,0,0" TextWrapping="Wrap" Text="Bacon Wrapped" VerticalAlignment="Top" Width="47" FontSize="8"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="10,47,0,0" TextWrapping="Wrap" Text="FIg &amp; pulled" VerticalAlignment="Top" Width="47" FontSize="8"/>
        <TextBox HorizontalAlignment="Left" Height="31" Margin="10,76,0,0" TextWrapping="Wrap" Text="Fig Panacotta" VerticalAlignment="Top" Width="47" FontSize="9"/>
        <Button Content="Back" HorizontalAlignment="Left" Height="14" Margin="5,112,0,0" VerticalAlignment="Top" Width="28" FontSize="8" Click="backToMenu"/>

    </Grid>
</Page>
